{
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "TS_NODE_PROJECT=tsconfig-for-webpack-config.json webpack-dev-server",
    "build": "TS_NODE_PROJECT=tsconfig-for-webpack-config.json NODE_ENV=production webpack",
    "lint": "eslint ./src  --ext .ts,.tsx",
    "preanalyze": "TS_NODE_PROJECT=tsconfig-for-webpack-config.json  NODE_ENV=production webpack --profile --json > stats.json",
    "analyze": "webpack-bundle-analyzer stats.json ./dist",
    "build-gh-pages": " yarn build && gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.2",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.19.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn tsc"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{json,js,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.1.1"
  }
}
